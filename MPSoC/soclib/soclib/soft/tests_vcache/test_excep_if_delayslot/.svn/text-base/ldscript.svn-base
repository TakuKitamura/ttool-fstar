ENTRY(_start)

SECTIONS
{
   . = 0xbfc00000;
   .text : {
      *(.text)
/* load .text2 one page after .text */
      . = ALIGN(0x1000) - 4;
      *(.text2)
   }
   .rodata : {
      *(.rodata)
      . = ALIGN(4);
   }
   . = 0x00000000;
   .data  : {
      *(.data)
   }
   .sdata : {
      *(.lit8)
      *(.lit4)
      *(.sdata)
   }
   _gp = .;
   . = ALIGN(4);
   _edata = .;
   .sbss  : {
      *(.sbss)
      *(.scommon)
   }
   .bss   : {
      *(.bss)
      *(COMMON)
   }
   . = ALIGN(4);
   _end = .;
   _heap = .;
   _stack = 0x00400000-16;
}
